{
  "name": "quiz-maker",
  "version": "1.0.0",
  "description": "A site for making and sharing custom quizzes!",
  "repository": "https://github.com/sarahkittyy/quizzy",
  "main": "build/backend/index.js",
  "scripts": {
    "dev-backend": "tsc-watch --build tsconfig.backend.json --pretty --onSuccess 'node .'",
    "build-backend": "tsc .",
    "dev-frontend": "webpack-dev-server",
    "build-frontend": "webpack --mode=production",
    "dev": "concurrently 'npm run dev-backend' 'npm run dev-frontend'",
    "build": "npm run build-backend && npm run build-frontend",
    "docker-start": "docker-compose up -d",
    "docker-stop": "docker-compose down",
    "docker-connect": "docker-compose run app /bin/bash || true"
  },
  "author": "Sarah Ohlin",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.5.35",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "app-root-path": "^3.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.3",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  }
}
